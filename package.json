{
  "name": "leetcode",
  "version": "1.0.0",
  "description": "Solutions for leetcode problems",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "lint:fix": "eslint \"**/*.js\" --fix",
    "test": "jest --env=node --findRelatedTests --bail --passWithNoTests"
  },
  "repository": "https://github.com/Lilybon/leetcode.git",
  "author": "Lilybon <zebra10029@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^4.2.5",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint:fix",
      "npm run test"
    ]
  }
}
